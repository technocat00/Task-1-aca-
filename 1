https://leetcode.com/problems/majority-element/submissions/1638005147

class Solution {
public:
    int majorityElement(vector<int>& nums) {
      unordered_map <int, int> freq; // i used unordred map to make it 0(1)
      int n= nums.size(); int k; 
      for(int i=0; i<n; i++){
        freq[nums[i]]++;   // counting the freq of unique numbers
      }
      for(auto &max: freq){
        if(max.second > n/2){   
            k= max.first;    // accessing it through iterating
        }
      }
      return k;
    }
};
