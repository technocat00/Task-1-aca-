https://leetcode.com/problems/remove-duplicates-from-sorted-array/submissions/1639944855

class Solution {
public:
    int removeDuplicates(vector<int>& nums) {
        set<int> unique; // used set to store unique elements
        //0(nlogn)

        for(int i=0; i<nums.size(); i++){
           unique.insert(nums[i]); 
        }
        int sum=0;
        for(auto value: unique){
            nums[sum]=value;                                
            sum++;
        }
        return sum;
    }
};
