class Solution {
public:
    void sortColors(vector<int>& nums) {
        multiset<int> s; //0(nlogn)   // to sort without removing
        for(int i=0; i<nums.size(); i++){
            s.insert(nums[i]);   // storing
        }
        int i=0;
        for(auto &value: s){
            nums[i]= value;   // modifying nums
            cout << nums[i];
            i++;
        }
    }
};

link- https://leetcode.com/problems/sort-colors/submissions/1639973608
